{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "tabWidth": 2,
  "printWidth": 150,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSameLine": true,
  "endOfLine": "auto",
  "importOrder": ["^@nestjs", "^axios", "^[./]"],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
